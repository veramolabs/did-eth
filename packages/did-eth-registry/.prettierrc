{
  "tabWidth": 2,
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "es5",
  "semi": false,
  "plugins": [
    "prettier-plugin-solidity"
  ],
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false,
        "useTabs": false,
        "bracketSpacing": true
      }
    }
  ]
}